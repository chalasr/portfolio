<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Robin\PortfolioBundle\Entity\Project" table="projects" repository-class="Robin\PortfolioBundle\Entity\ProjectRepository">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="outils" type="text" column="outils" nullable="false"/>
    <field name="class" type="string" length="255" column="class" nullable="false"/>
  </entity>
</doctrine-mapping>
